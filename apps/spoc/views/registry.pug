.row
  .col-lg-3.col-xs-6
    // small box
    .small-box.bg-green
      .inner
        h3
          | #{numfiles}
        |              
        p Files
      |             
      .icon
        i.ion.ion-laptop
      |             
      a.small-box-footer.tomanageapp(href='#')
        | Upload Files 
        i.fa.fa-arrow-circle-right
  // ./col
.row
  .col-lg-12
    .box.box-primary
      .box-header.ui-sortable-handle(style='cursor: move;')
        i.fa.fa-user.fa-fw
        |               
        h3.box-title Registry files
        |               
        .box-tools.pull-right
          button.btn.bg-teal.btn-sm(type='button', data-widget='collapse')
            i.fa.fa-minus
          |                 
          button.btn.bg-teal.btn-sm(type='button', data-widget='remove')
            i.fa.fa-times  
      .box-body 
        table.table.table-condensed
          tbody
            tr
              th #
              |
              th Surveyor
              |                   
              th Plan Name
              |                   
              th Plan Type
              |                   
              th Scale
              |                   
              th Coordinate Sys
              | 
              th Location
              |  
              th
              |    
            each element in myfiles
              tr
                td=element.ind
                |
                td=element.surveyor
                |
                td=element.planname
                |
                td=element.plantype
                |
                td=element.Scale
                |
                td=element.coordinatesys
                |
                td=element.location
                |
                if element.registry == 0
                  td
                    button.button.btn.btn-block.btn-lg.btn-primary.toregistrysubmitbtn(type='button' chref=element.ind)
                      | Submit
                else
                  td.btn-success
                    | Submitted
                

    //.box

script.
  $(".uploadfilesstartbtn").click(function(){loaduploadform();})
  $(".uploadcsvstartbtn").click(function(){uploadcsvstart();})
  $(".uploaddwgstartbtn").click(function(){uploaddwgstart();})
  $(".toregistrysubmitbtn").click(function(){registrysubmit($(this).attr("chref"));})
