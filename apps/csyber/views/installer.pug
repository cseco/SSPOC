.box.box-primary
  .box-header.ui-sortable-handle(style='cursor: move;')
    i.fa.fa-gear.fa-fw
    |               
    h3.box-title Install application
    |               
    .box-tools.pull-right
      button.btn.bg-teal.btn-sm(type='button', data-widget='collapse')
        i.fa.fa-minus
      |
  .box-body 
    p
      strong
        | Install this application
      br
      |!{ html }
      br
      |
      button.btn.btn-success.installapp.pull-right
        |  Install
      button.btn.btn-primary.pull-left.backbtn
        span.fa.fa-angle-double-left.custom
        |  Back
      |
    |

script.
  $(".backbtn").click(function(){loadlastpage();});
  $(".installapp").click(function(){installapp("install");});