if(!less)
  doctype html
  html
    head
      meta(charset='utf-8')
      |   
      meta(http-equiv='X-UA-Compatible', content='IE=edge')
      |   
      title #{appname} | #{title}
      // Tell the browser to be responsive to screen width
      meta(content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no', name='viewport')

      //link(rel='stylesheet', href=rooturl+'userfrosting/all.css')

      // Bootstrap 3.3.6
      link(rel='stylesheet', href=rooturl+'AdminLTE/bootstrap/css/bootstrap.min.css')
      // Font Awesome
      link(rel='stylesheet', href=href=rooturl+'assets-raw/core/assets/vendor/font-awesome/css/font-awesome.css')
      // Ionicons
      link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css')
      // Theme style
      link(rel='stylesheet', href=rooturl+'AdminLTE/dist/css/AdminLTE.min.css')
      //
        AdminLTE Skins. Choose a skin from the css/skins
        folder instead of downloading all of them to reduce the load.
      link(rel='stylesheet', href=rooturl+'AdminLTE/dist/css/skins/_all-skins.min.css')
      // iCheck
      link(rel='stylesheet', href=rooturl+'AdminLTE/plugins/iCheck/flat/blue.css')
      // Morris chart
      link(rel='stylesheet', href=rooturl+'AdminLTE/plugins/morris/morris.css')
      // jvectormap
      link(rel='stylesheet', href=rooturl+'AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.css')
      // Date Picker
      link(rel='stylesheet', href=rooturl+'AdminLTE/plugins/datepicker/datepicker3.css')
      // Daterange picker
      link(rel='stylesheet', href=rooturl+'AdminLTE/plugins/daterangepicker/daterangepicker.css')
      // bootstrap wysihtml5 - text editor
      link(rel='stylesheet', href=rooturl+'AdminLTE/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css')
      // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
      // WARNING: Respond.js doesn't work if you view the page via file://
      //if lt IE 9
        script(src='https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js')
        |   
        script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
      link(rel='stylesheet', href=rooturl+'alertifyjs/css/alertify.min.css')
      link(rel='stylesheet', href=rooturl+'alertifyjs/css/themes/default.min.css')
    //body.hold-transition.skin-blue.sidebar-mini
    body(class="hold-transition "+ skin + " sidebar-mini")

    include loggedinheader
    .wrapper
      include leftsidebar
      // Content Wrapper. Contains page content
      .content-wrapper
        // Content Header (Page header)
        include ../flash
        section.content-header
          h1
            | &nbsp;
          |       
          ol.breadcrumb
            li
              a(href='#')
                i.fa.fa-desktop
                |  #{application}
            |         
            li.active.thisapp #{section}
              li.active.thisdashboard #{section}
        block content
      block footer
if(less)
  block content